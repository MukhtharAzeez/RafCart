<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="RAFCART - Multipurpose eCommerce HTML Template">
    <meta name="author" content="Programming Kit">
    <title>RAFCART - Multipurpose eCommerce HTML Template</title>
    <link rel="shortcut icon" href="images/favicon.png" type="image/x-icon">

    <!-- all css -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/jquery-ui.css">
    <link rel="stylesheet" href="css/slick.css">
    <link rel="stylesheet" href="css/line-awesome.css">
    <link rel="stylesheet" href="css/nice-select.css">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/responsive.css">
</head>

<body>

    <!-- Preloader -->
    <div class="preloader">
         <img src="https://res.cloudinary.com/dr2hks7gt/image/upload/v1668106009/La%20Bonnz/Walk_xjkfby.gif" alt="preloader">
    </div>


   

    <!-- breadcrumbs -->
    <div class="container">
        <div class="breadcrumbs">
            <a href="/"><i class="las la-home"></i></a>
            <a href="/view-account" class="active">My Account</a>
        </div>
    </div>

    <!-- account -->
    <div class="my_account_wrap section_padding_b">
        <div class="container">
            <div class="row">
                <!--  account sidebar  -->
                {{>user_account_common}}
                <!-- account content -->
                <div class="col-lg-9">
                    <div class="account_cont_wrap">
                        <div class="profile_info_wrap">
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="single_prof_info shadow_sm">
                                        <div class="prof_info_title">
                                            <h4>Personal Profile</h4>
                                            <a href="/edit-user-profile">Edit</a>
                                        </div>

                                        <div class="prfo_info_cont">
                                            <p class="text-semibold">{{userDetails.userName}}</p>
                                            <p>{{userDetails.email}}</p>
                                            <p>{{userDetails.phone}}</p>
                                        </div>
                                    </div>
                                </div>


                                
                                
                                
                               
                                {{#each userDetails.address}}
                                <div class="col-lg-4">
                                    <div class="single_prof_info shadow_sm mb-0">
                                        <div class="prof_info_title">
                                            
                                            <a href="/account-edit-address?index={{@index}}">Edit</a>
                                            <a href="/account-delete-address?index={{@index}}">Delete</a>
                                        </div>
                                        <div class="prfo_info_cont">
                                            <p class="text-semibold">{{this.fullName}}</p>
                                            <p>{{this.country}}</p>
                                            <p>{{this.state}},{{this.city}}</p>
                                            <p>{{this.address}}</p>
                                            <p>{{this.phone}}</p>
                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                                <div class="col-lg-4">
                                    <div class="single_prof_info shadow_sm mb-0">
                                        <div class="prof_info_title" style="padding-top: 80px;">
                                            <a href="/add-address">
                                                <span class="icon" style="margin-left: 90px; color: red; font-size: 40px;" id="addAdress">
                                                <i class="la la-plus-circle"oooooooooooooooooooooooooooo></i>
                                                </span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                       
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- copyright -->
    <div class="copyright_wrap">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-6">
                    <p class="copyright_text">Â© RAFCART - All Rights Reserved</p>
                </div>
                <div class="col-md-6">
                    <div class="payment_method">
                        <img loading="lazy" src="images/payment-method.png" alt="payment method">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- all js -->
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/jquery-3.5.1.min.js"></script>
    <script src="js/jquery-ui.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/jquery.nice-select.min.js"></script>
    <script src="js/main.js"></script>
</body>

</html>