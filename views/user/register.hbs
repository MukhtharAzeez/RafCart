<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="RAFCART - Multipurpose eCommerce HTML Template">
    <meta name="author" content="Programming Kit">
    <title>RAFCART - Multipurpose eCommerce HTML Template</title>

    <!-- all css -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/jquery-ui.css">
    <link rel="stylesheet" href="css/slick.css">
    <link rel="stylesheet" href="css/line-awesome.css">
    <link rel="stylesheet" href="css/nice-select.css">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/responsive.css">
</head>

<body>

    <!-- Preloader -->
    <div class="preloader">
         <img src="images/walk.gif" alt="preloader">
    </div>

    <!-- top header -->


    <!-- breadcrumbs -->
    <div class="container">
        <div class="breadcrumbs">
            <a href="index-1.html"><i class="las la-home"></i></a>
            <a href="#" class="active">Register</a>
        </div>
    </div>

    <!--register wrapper-->
    <div class="register_wrap section_padding_b">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-xl-5 col-lg-7 col-md-9">
                    <div class="logo ps-5 d-flex">
                        <a href="/">
                            <img loading="lazy"
                                src="/images/svg/LaBonnz.svg"
                                alt="logo">
                        </a>
                    </div>
                    <div class="register_form padding_default shadow_sm">
                        <h4 class="title_2">Create an account</h4>
                        <p class="mb-4 text_md">Register here if you are a new customer.</p>
                        <form action="/register" id="registrationForm" method="post">
                            <div class="row">
                                <div class="col-12">
                                    <div class="single_billing_inp">
                                        <label>Full Name <span>*</span></label>
                                        <input type="text" placeholder="type username" name="userName" required
                                            id="fullName">
                                        <span id="fullNameError" style="color: red; font-size: 11px;">Only characters
                                            are valid!</span>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="single_billing_inp">
                                        <label>Email Address <span>*</span></label>
                                        <input type="email" placeholder="example@mail.com" name="email" required
                                            id="email">
                                        <span id="emailError" style="color: red; font-size: 11px;">type email!</span>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="single_billing_inp">
                                        <label>Phone Number <span>*</span></label>
                                        <input type="text" placeholder="type phone number" name="phone" required
                                            id="phone">
                                        <span id="phoneError" style="color: red; font-size: 11px;">enter valid phone
                                            number!</span>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="single_billing_inp">
                                        <label>Password <span>*</span></label>
                                        <input type="password" placeholder="type password" name="password" required
                                            id="password">
                                        <span id="passwordError" style="color: red; font-size: 11px;">password must be
                                            atleast 8 & no special characters are allowed!</span>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="single_billing_inp">
                                        <label>Confirm Password <span>*</span></label>
                                        <input type="password" placeholder="confirm your password"
                                            name="confirmPassword" required id="confirmPassword">
                                        <span id="confirmPasswordError" style="color: red; font-size: 11px;">Confirm
                                            password and password must be same!</span>
                                    </div>
                                </div>
                                <div class="col-12 mt-2">
                                    <div class="custom_check check_2 d-flex align-items-center">
                                        <input type="checkbox" class="check_inp" hidden id="save-default">
                                        <label for="save-default">I have read and agree to the <a
                                                href="terms-condition.html" class="text-color">terms & conditions</a>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-12 mt-3" id="SubmitButton">
                                    <button type="submit" class="default_btn xs_btn rounded px-4 d-block w-100">create
                                        account</button>
                                </div>

                            </div>
                        </form>

                        <div class="dif_regway my-3">
                            <span class="txt">Or singup in with</span>
                        </div>

                        <div class="d-flex">
                            <button class="default_btn xs_btn rounded px-4 d-block w-50 text-capitalize bg-facebook">
                                <i class="fab fa-facebook-f me-2"></i> Facebook
                            </button>
                            <button class="default_btn xs_btn rounded px-4 d-block w-50 ms-3 text-capitalize bg-google">
                                <i class="fab fa-google me-2"></i> Google
                            </button>
                        </div>

                        <p class="text-center mt-3 mb-0">Already have an account.? <a href="/login"
                                class="text-color">Login Now</a></p>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- footer area -->

    <!-- copyright -->
    <div class="copyright_wrap">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-6">
                    <p class="copyright_text">Â© RAFCART - All Rights Reserved</p>
                </div>
                <div class="col-md-6">
                    <div class="payment_method">
                        <img loading="lazy" src="images/payment-method.png" alt="payment method">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- all js -->
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/jquery-3.5.1.min.js"></script>
    <script src="js/jquery-ui.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/jquery.nice-select.min.js"></script>
    <script src="js/main.js"></script>
    <script src="js/signupValidation.js"></script>
</body>

</html>