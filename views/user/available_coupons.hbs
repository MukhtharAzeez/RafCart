    <div class="my_account_wrap section_padding_b">
        <div class="container">
            <div class="row">
                <!--  account sidebar  -->
                {{>user_account_common}}
                <!-- account content -->
                <div class="col-lg-9">
                    <div class="acprof_info_wrap voucher shadow_sm">
                        <div class="container">
                            <h1>Coupons</h1>
                            <div class="row">
                                {{#each coupons}}
                                {{#if this.available}}
                                <div class="col-6 card ms-5 mb-5 ">
                                    <section class="date">
                                        <time datetime="23th feb">
                                            {{#if this.percentage}}
                                            <span></span><span>{{this.discountValue}}%</span>
                                            {{else if this.freeShipping}}
                                            <span></span><span style="font-size: 11px;">Free Delivery</span>
                                            {{else}}
                                            <span></span><span>₹{{this.discountValue}}</span>
                                            {{/if}}
                                        </time>
                                    </section>
                                    <section class="card-cont">
                                        <small>{{this.type}}</small>
                                        <h3>{{this.code}}</h3>
                                        <div class="even-date">
                                            <i class="fa fa-calendar"></i>
                                            <time>
                                                <span>{{this.startDate}}</span>
                                                <span>To</span>
                                                <span>{{this.expiryDate}}</span>
                                                <span>{{this.usageLimit}} Left</span>
                                            </time>
                                        </div>
                                        {{#if this.used}}
                                        <a><i class="fa-solid fa-check">Used</i></a>
                                        {{else}}
                                        <a href="/view-cart?code={{this.code}}">use</a>
                                        {{/if}}
                                    </section>
                                </div>
                                {{/if}}
                                {{/each}}
                            </div>
                        </div>
                            
                    </div>

