                <form action="/admin/add-product" method="post" id="edit-product" enctype="multipart/form-data">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="name" class="form-label">Name</label>
                        <input type="text" class="form-control" id="name" name="name" required>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="category" class="form-label">Category</label>
                        <select name="category" class="form-control" id="category" required>
                          <option value="">Select Category</option>
                          {{#each category}}
                          <option value="{{this.name}}">{{this.name}}</option>
                          {{/each}}
                        </select>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="stock" class="form-label">Stock</label>
                        <input type="number" class="form-control" id="stock" name="stock" required>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="tags" class="form-label">Tags</label>
                        <input type="text" class="form-control" id="tags" name="tags" required>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="price" class="form-label">Price</label>
                        <input type="number" class="form-control" id="price" name="price" required>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="mb-3">
                        <label for="price" class="form-label">Discount Price</label>
                        <input type="number" class="form-control" id="price" name="discount_price" required>
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div>
                        <label for="formFile-3-invalid" class="form-label">Choose Images</label><input type="file"
                          class="form-control is-invalid" id="formFile-3-invalid" name="image1" />
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div>
                        <label for="formFile-3-invalid" class="form-label">Choose Images</label><input type="file"
                          class="form-control is-invalid" id="formFile-3-invalid" name="image1" />
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div>
                        <label for="formFile-3-invalid" class="form-label">Choose Images</label><input type="file"
                          class="form-control is-invalid" id="formFile-3-invalid" name="image1" />
